{
    "configurations": [
        {
            "name": "nrfCC",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/src"
            ],
            "defines": [
                "BOARD_PCA10040",
                "CONFIG_GPIO_AS_PINRESET",
                "FREERTOS",
                "NIMBLE_CFG_CONTROLLER",
                "NRF52",
                "NRF52832",
                "NRF52832_XXAA",
                "NRF52_PAN_12",
                "NRF52_PAN_15",
                "NRF52_PAN_20",
                "NRF52_PAN_31",
                "NRF52_PAN_36",
                "NRF52_PAN_51",
                "NRF52_PAN_54",
                "NRF52_PAN_55",
                "NRF52_PAN_58",
                "NRF52_PAN_64",
                "NRF52_PAN_74",
                "OS_CPUTIME_FREQ",
                "__HEAP_SIZE=4096",
                "__STACK_SIZE=1024"
            ],
            "compilerPath": "${env:ARM_NONE_EABI_TOOLCHAIN_PATH}/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-arm",
            "configurationProvider": "ms-vscode.cpp-tools",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}